{
    "name": "skript-vscode",
    "displayName": "skript-vscode",
    "description": "A direct port of https://github.com/Skript-Atom/Language-Skript to Visual Studio Code",
    "version": "2.1.3",
    "publisher": "konamidude",
    "repository": {
      "type" : "git",
      "url" : "https://github.com/Penple03/skript-vscode.git"
    },
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "skript",
            "aliases": ["Skript", "skript"],
            "extensions": [".sk"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "skript",
            "scopeName": "source.skript",
            "path": "./syntaxes/skript.tmLanguage.json"
        }]
    }
}
